{
  "name": "<%= appname %>",
  "version": "0.1.0",
  "description": "My Angular2-TypeScript App",
  "keywords": [
    "angular2",<% if (systemjs) { %>
    "systemjs",<% } %>
    "typescript"<% if (webpack) { %>,
    "webpack"<% } %>
  ],
  "scripts": {
    "postinstall": "./node_modules/.bin/typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "start": "gulp"<% if (systemjs) { %>,
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w"<% } if (webpack) { %>,
    "test": "karma start",
    "build": "gulp build"<% } %>
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.3",
    "@angular/compiler": "^2.0.0-rc.3",
    "@angular/core": "^2.0.0-rc.3",<% if (angularPackages['@angular/http']) { %>
    "@angular/http": "^2.0.0-rc.3",<% } %>
    "@angular/platform-browser": "^2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.3",
    "@angular/router-deprecated": "^2.0.0-rc.2",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.19",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-shell": "^0.5.2",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.1",
    "concurrently": "^2.0.0",<% if (systemjs) { %>
    "lite-server": "^2.2.2",
    "systemjs": "0.19.27",<% } %>
    "tslint": "^3.12.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.0" <% if (webpack) { %>,
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"<% } %>
  }
}
